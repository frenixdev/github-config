[
  {
    "key": "ctrl+p",
    "command": "workbench.action.showCommands"
  },
  // {
  //   "key": "ctrl +x",
  //   "command": "workbench.view.extensions"
  // },
  //settings
  {
    "key": "alt+ctrl+,",
    "command": "workbench.action.openSettings"
  },
  //reset zoom
  {
    "key": "ctrl+0",
    "command": "workbench.action.zoomReset"
  },
  {
    "key": "escape",
    "command": "notifications.hideToasts",
    "when": "notificationToastsVisible"
  },
  //jump line
  {
    "key": "ctrl +g",
    "command": "workbench.action.gotoLine"
  },
  //jump to funciton code navigator in current file
  {
    "key": "ctrl+r",
    "command": "workbench.action.gotoSymbol"
  },
  //code navigator / find functions vaiables classes in all files of the root folder
  {
    "key": "ctrl+shift+r",
    "command": "workbench.action.showAllSymbols"
  },
  {
    "key": "ctrl+1",
    "command": "workbench.action.openEditorAtIndex1",
    "when": "!terminalFocus"
  },
  {
    "key": "ctrl+2",
    "command": "workbench.action.openEditorAtIndex2",
    "when": "!terminalFocus"
  },
  {
    "key": "ctrl+3",
    "command": "workbench.action.openEditorAtIndex3",
    "when": "!terminalFocus"
  },
  {
    "key": "ctrl+4",
    "command": "workbench.action.openEditorAtIndex4",
    "when": "!terminalFocus"
  },
  {
    "key": "ctrl+5",
    "command": "workbench.action.openEditorAtIndex5",
    "when": "!terminalFocus"
  },
  {
    "key": "ctrl+6",
    "command": "workbench.action.openEditorAtIndex6",
    "when": "!terminalFocus"
  },
  {
    "key": "ctrl+7",
    "command": "workbench.action.openEditorAtIndex7",
    "when": "!terminalFocus"
  },
  {
    "key": "ctrl+8",
    "command": "workbench.action.openEditorAtIndex8",
    "when": "!terminalFocus"
  },
  {
    "key": "ctrl+9",
    "command": "workbench.action.openEditorAtIndex9",
    "when": "!terminalFocus"
  },
  //explorer focus enable
  {
    "key": "ctrl+h",
    "command": "workbench.action.focusSideBar",
    "when": "editorTextFocus || terminalFocus"
  },
  //explorer focus disable if focused

  {
    "key": "ctrl+h",
    "command": "workbench.view.search.focus",
    "when": "activeViewlet == 'workbench.view.search' && !inputFocus"
  },
  //switch focus to search tab but
  //first open global search by ctrl+shi ft+f
  {
    "key": "ctrl+h",
    "command": "workbench.action.focusActiveEditorGroup",
    "when": "editorIsOpen && !editorTextFocus && !view.terminal.visible"
  },
  {
    "key": "ctrl+k",
    "command": "search.focus.nextInputBox",
    "when": "inSearchEditor && inputBoxFocus || inputBoxFocus && searchViewletVisible"
  },

  {
    "key": "ctrl+l",
    "command": "search.focus.previousInputBox",
    "when": "inSearchEditor && inputBoxFocus || inputBoxFocus && searchViewletVisible && !searchInputBoxFocus"
  },

  {
    "key": "alt+ctrl+;",
    "command": "search.action.collapseSearchResults"
  },
  {
    "key": "alt+ctrl+'",
    "command": "search.action.expandSearchResults"
  },
  {
    "key": "alt+ctrl+0",
    "command": "search.action.focusSearchList"
  },

  //context menu
  {
    "key": "alt+enter",
    "command": "editor.action.showContextMenu",
    "when": "editorTextFocus"
  },
//copy lined down or below
  {
    "key": "ctrl+u",
    "command": "editor.action.copyLinesDownAction",
    "when": "editorTextFocus && !editorReadonly"
  },
  //formatter
  {
    "key": "alt+ctrl+k",
    "command": "editor.action.formatDocument",
    "when": "editorHasDocumentFormattingProvider && editorTextFocus && !editorReadonly && !inCompositeEditor"
  },

  //tag wrapper
  {
    "key": "ctrl+m ctrl+w",
    "command": "editor.emmet.action.wrapWithAbbreviation",
    "when": "editorTextFocus"
  },
  //change tag name
  {
    "key": "ctrl+m ctrl+r",
    "command": "editor.emmet.action.updateTag",
    "when": "editorTextFocus"
  },

//delete tag line
// if its wrapper the wrapper would get deleted not the child
  {
    "key": "ctrl+m ctrl+backspace",
    "command": "editor.emmet.action.removeTag",
    "when": "editorTextFocus"
  },
  //jump between opning tag to closing tag
  {
    "key": "ctrl+m ctrl+m",
    "command": "editor.emmet.action.matchTag",
    "when": "editorTextFocus"
  },
//fold parent bracket
  {
    "key": "ctrl+[",
    "command": "editor.fold",
    "when": "editorTextFocus && foldingEnabled"
  },
  //unfold parent brackets
  {
    "key": "ctrl+]",
    "command": "editor.unfold",
    "when": "editorTextFocus && foldingEnabled"
  },
  //fold all globally
  {
    "key": "alt+ctrl+[",
    "command": "editor.foldAll",
    "when": "editorTextFocus && foldingEnabled"
  },
  //unfold all globaly
  {
    "key": "alt+ctrl+]",
    "command": "editor.unfoldAll",
    "when": "editorTextFocus && foldingEnabled"
  },
  //fold all  block scope brackets
  {
    "key": "ctrl+m ctrl+[",
    "command": "editor.foldRecursively",
    "when": "editorTextFocus && foldingEnabled"
  },
  //unfold all  block scope brackets

  {
    "key": "ctrl+m ctrl+]",
    "command": "editor.unfoldRecursively",
    "when": "editorTextFocus && foldingEnabled"
  },
      {
        "key": "ctrl+n",
        "command": "workbench.action.terminal.new",
        "when": "terminalFocus"
    },
//collaps all opened folder inexplorer
  {
    "key": "ctrl+\\",
    "command": "workbench.files.action.collapseExplorerFolders"
  },
  //show active folder in explorer
  {
    "key": "alt+\\",
    "command": "workbench.files.action.showActiveFileInExplorer"
  },
//new file
  {
    "key": "ctrl+n",
    "command": "explorer.newFile",
    "when": "explorerViewletVisible && filesExplorerFocus && !inputFocus"
  },
  //new folder
  {
    "key": "shift+ctrl+n",
    "command": "explorer.newFolder",
    "when": "explorerViewletVisible && filesExplorerFocus && !inputFocus"
  },
  {
    "key": "ctrl+e",
    "command": "workbench.view.explorer",
    "when": "viewContainer.workbench.view.explorer.enabled"
  },
  {
  "key": "ctrl+t",
  "command": "workbench.action.terminal.toggleTerminal"
}
,
  //terminal close
  {
    "key": "Escape",
    "command": "workbench.action.terminal.toggleTerminal",
    "when": "terminalFocus"
  },

  //terminal  kill
  {
    "key": "ctrl+w",
    "command": "workbench.action.terminal.kill",
    "when": "terminalFocus"
  },
